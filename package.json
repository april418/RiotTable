{
  "name": "riot-table",
  "version": "0.1.0",
  "description": "Riot.javascripts table example",
  "main": "example/server/javascripts/index.js",
  "browser": "src/riot-table.tag",
  "scripts": {
    "prestart": "npm run init & npm run build",
    "start": "node example/server/javascripts/index.js & npm run watch",
    "restart": "npm stop && npm start",
    "init": "npm-run-all --parallel init:*",
    "init:directory": "npm-run-all --parallel make:directory:*",
    "make:directory:javascripts": "if [ ! -e example/public/javascripts ]; then mkdir example/public/javascripts; fi",
    "make:directory:templates": "if [ ! -e example/public/templates ]; then mkdir example/public/templates; fi",
    "make:directory:stylesheets": "if [ ! -e example/public/stylesheets ]; then mkdir example/public/stylesheets; fi",
    "build": "npm-run-all --parallel build:*",
    "build:tags": "browserify example/client/coffeescripts/index.coffee > example/public/javascripts/index.js",
    "build:pug": "pug --pretty --no-debug --out example/public/templates/ example/client/templates/",
    "build:sass": "node-sass --output example/public/stylesheets/ example/client/stylesheets/",
    "build:server": "coffee --output example/server/javascripts/ example/server/coffeescripts/",
    "watch": "npm-run-all --parallel watch:*",
    "watch:client": "watch 'npm run build:tags' src/ example/client/coffeescripts/",
    "watch:pug": "watch 'npm run build:pug' example/client/templates/",
    "watch:sass": "watch 'npm run build:sass' example/client/stylesheets/",
    "watch:server": "watch 'npm run build:server' example/server/coffeescripts/"
  },
  "repository": "https://github.com/april418/RiotTable.git",
  "author": "april418",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "riotify",
        {
          "type": "coffeescript",
          "expr": true,
          "template": "pug",
          "style": "sass"
        }
      ],
      [
        "coffeeify"
      ]
    ]
  },
  "dependencies": {
    "coffeeify": "^2.1.0",
    "materialize-css": "^0.97.8",
    "pug": "^2.0.0-beta8",
    "riot": "^3.0.7",
    "riot-compiler": "^3.1.2",
    "riotify": "^2.0.0"
  }
}
